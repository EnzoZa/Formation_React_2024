{
  "usesTypeScript": true,
  "usesStyledComponents": false,
  "usesCssModule": true,
  "cssPreprocessor": "css",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true
    },
    "hook": {
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true,
      "customTemplates":{
        "component": "grc_template/hookedFunction/TemplateName.tsx"
      }
    },
    "layout": {
      "path": "src/components/layout",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true,
      "customTemplates":{
        "component": "grc_template/layout/TemplateName.tsx",
        "story": "grc_template/layout/readme.md"
      }
    },
    "ui": {
      "path": "src/components/ui",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true,
      "customTemplates":{
        "component": "grc_template/ui/TemplateName.tsx"
      }
    }
  }
}